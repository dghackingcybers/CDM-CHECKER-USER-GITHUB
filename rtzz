                    if (message.content == "cancelar") {

                        coletor.stop('Collector stopped manually');

                        let errado = new Discord.EmbedBuilder()
                            .setDescription(`OperaÃ§Ã£o cancelada com sucesso.`)
                            .setColor(`${colorNB}`)

