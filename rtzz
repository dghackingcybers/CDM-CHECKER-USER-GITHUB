                            let MENSAGEM = await membro.send({ files: [imagem.attachment] });

                            await db.set(`setimageReg_${b.guild.id}`, MENSAGEM.attachments.first().url);

                            let titulo = await db.get(`tituloReg_${b.guild.id}`);
                            if (!titulo) titulo = 'TÃ­tulo'
