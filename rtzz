                        let diasAdicionado = parse(data) + parse(`${diasBot} dias`);

                        const object2 = `{
                                
                            "diasBot": "${diasAdicionado}",
                            "clientID": "${client.user.id}"
                        }`

                        if (v) {

                            await db.delete(`diasbot_${client.user.id}`);
                            await db.push(`diasbot_${client.user.id}`, JSON.parse(object2));

