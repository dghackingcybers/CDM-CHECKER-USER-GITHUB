                if (ultimoReset) {

                    ultimoReset = `\`${moment(ultimoReset).fromNow()}\``

                } else {

                    ultimoReset = `\`NÃ£o foi resetado atÃ© o momento.\``
                }

                const embedTempo = new Discord.EmbedBuilder()
