                            rcar = `Nenhum`;
                        } else {
                            rcar = `<@&${cargoVip.id}>`;  // Corrigido para mencionar o cargo corretamente
                        }

                        if (!callVip) {
                            cal = `Nenhum`;
                        } else if (callVip instanceof Discord.VoiceChannel) {
                            cal = `<#${callVip.id}>`;  // Menciona o canal de voz corretamente
                        } else {
                            cal = `Nenhum`;
                        }

