                    let encerraovip = await db.get(`encerravip_${ee.id}`);

                    if (!encerraovip) {

                        coletor.stop('Collector stopped manually');

                        let errado = new Discord.EmbedBuilder()
                            .setDescription(`${b.member}, o membro mencionado nÃ£o possui VIP.`)
