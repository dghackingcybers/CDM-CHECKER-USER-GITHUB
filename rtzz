                        if (vls) {

                            for (let pd of vls) {

                                embedVip.addFields({
                                    name: `${client.xx.vips} ` + pd.vipnome,
                                    value: `${client.xx.duracao} DuraÃ§Ã£o: ` + `\`${pd.diasvip} Dias.\``
                                })

                            }

                        }

                        embedVip.addFields(

                            { name: `Logs Vip`, value: `${logs}`, inline: false }

                        )
                        await MESSAGE.edit({ embeds: [embedVip] });
                    }

                })

            }

            if (b.customId == 'gerenciarVip') {

                let embedmsgs = new Discord.EmbedBuilder()
                    .setDescription(`Envie no chat o (@usuario/id) do membro VIP para gerenciar\nPara cancelar a operaÃ§Ã£o digite: \`cancelar\``)
                    .setColor(`${colorNB}`)

