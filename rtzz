                if (statusEmbed === true) {

                    emojiStatusBlacklist = `1119444704178745464`;
                    emojiStatusBlacklistEmbed = `> ${client.xx.ativado} Ativado`;

                } else {

                    emojiStatusBlacklist = `1119452618394177626`;
                    emojiStatusBlacklistEmbed = `> ${client.xx.desativado} Desativado`;

